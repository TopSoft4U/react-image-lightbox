{
  "name": "@topsoft4u/react-image-lightbox",
  "version": "6.0.2",
  "description": "A lightbox component for React.js",
  "scripts": {
    "build": "rollup -c",
    "build:demo": "parcel build examples/cats/index.html --out-dir build --public-url ./",
    "clean": "rimraf dist style.css style.css.map",
    "clean:demo": "rimraf build",
    "deps:check": "npx npm-check-updates --dep prod,dev,optional",
    "deps:update": "npx npm-check-updates --dep prod,dev,optional -u",
    "start": "parcel examples/cats/index.html",
    "lint": "eslint .",
    "prettier": "prettier --write \"./**/*.{md,js,css}\" \"!./{build,dist}/**\" \"!./style.css*\"",
    "prepublishOnly": "yarn run lint && yarn run test && yarn run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "deploy": "yarn run build:demo && gh-pages -d build",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "typings": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts",
    "style.css",
    "style.css.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TopSoft4U/react-image-lightbox.git"
  },
  "homepage": "https://TopSoft4U.github.io/react-image-lightbox",
  "bugs": "https://github.com/TopSoft4U/react-image-lightbox/issues",
  "authors": [
    "Chris Fritz"
  ],
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not ie_mob > 0",
    "not ie > 0"
  ],
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "16.x || 17.x",
    "react-dom": "16.x || 17.x",
    "classnames": "2.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.7.2",
    "babel-jest": "^27.0.6",
    "classnames": "^2.3.1",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-17-updated": "^1.0.2",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "jest-enzyme": "^7.1.2",
    "npm-check-updates": "^12.1.0",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.3.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.0",
    "rollup": "^2.53.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "standard-version": "^9.3.2",
    "react-bootstrap": "^2.1.1",
    "bootstrap": "^5.1.3"
  },
  "keywords": [
    "react",
    "react-component",
    "image",
    "lightbox"
  ]
}
